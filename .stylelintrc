{
  "extends": "stylelint-config-recommended",
  "rules": {
    "at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
        "extends",
        "include",
        "mixin",
        "if",
        "else",
        "function",
        "return",
        "warn"
      ]
    }],
     rule-empty-line-before:["always-multi-line"]
  }
}